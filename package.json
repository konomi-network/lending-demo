{
  "name": "konomi-compound-demo",
  "version": "0.1.1",
  "private": true,
  "license": "Unlicense",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "CI=true react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "lint:ci": "eslint src/**/*.js --max-warnings=0",
    "lint:fix": "eslint --fix src/**/*.js",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "dependencies": {
    "@polkadot/api": "^4.13.1",
    "@polkadot/extension-dapp": "^0.38.4",
    "@polkadot/keyring": "^6.7.1",
    "@polkadot/types": "^4.13.1",
    "@polkadot/ui-keyring": "^0.79.1",
    "@polkadot/ui-settings": "^0.79.1",
    "@polkadot/util": "^6.7.1",
    "@polkadot/util-crypto": "^6.7.1",
    "lodash": "^4.17.21",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.5",
    "react": "^16.14.0",
    "react-app-rewired": "^2.1.8",
    "react-cookie": "^4.0.3",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-node": "11.x",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^3.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard"
    ],
    "plugins": [
      "only-warn"
    ],
    "globals": {
      "BigInt": true
    },
    "rules": {
      "semi": [
        1,
        "always"
      ],
      "no-extra-semi": 1
    }
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
