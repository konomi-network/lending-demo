{
  "CUSTOM_TYPES": {
    "Address": "AccountId",
    "AssetId": "u64",
    "FeeRate": "u64",
    "LookupSource": "AccountId",
    "User": {
      "borrowLimit": "Balance",
      "supplyBalance": "Balance",
      "debtBalance": "Balance"
    },
    "Pool": {
      "enabled": "bool",
      "canBeCollateral": "bool",
      "asset": "AssetId",
      "supply": "Balance",
      "debt": "Balance",
      "safeFactor": "u128",
      "closeFactor": "u128",
      "discountFactor": "u128",
      "totalSupplyIndex": "u128",
      "totalDebtIndex": "u128",
      "lastUpdated": "BlockNumber",
      "utilizationFactor": "u128",
      "initialInterestRate": "u128"
    },
    "UserSupply": {
      "amount": "Balance",
      "index": "u128"
    },
    "UserDebt": {
      "amount": "Balance",
      "index": "u128"
    }
  },
  "PROVIDER_SOCKET": "ws://127.0.0.1:9944",
  "RPC": {
    "lending": {
      "supplyRate" :{
        "description": "get asset supply rate",
        "isSubscription": "false",
        "jsonrpc": "lending_supplyRate",
        "method": "supplyRate",
        "params": [
          {
            "name": "id",
            "type": "AssetId"
          },
          {
            "name": "at",
            "type": "Hash",
            "isOptional": true
          }
        ],
        "type": "u128"
      }
    },
    "debtRate" :{
      "description": "get asset debt rate",
      "isSubscription": "true",
      "jsonrpc": "lending_debtRate",
      "method": "debtRate",
      "params": [
        {
          "name": "id",
          "type": "AssetId"
        },
        {
          "name": "at",
          "type": "Hash",
          "isOptional": true
        }
      ],
      "section": "lending",
      "type": "u128"
    },
    "getUserInfo" :{
      "description": "get user info",
      "isSubscription": "true",
      "jsonrpc": "lending_getUserInfo",
      "method": "getUserInfo",
      "params": [
        {
          "name": "user",
          "type": "AccountId"
        },
        {
          "name": "at",
          "type": "Hash",
          "isOptional": true
        }
      ],
      "section": "lending",
      "type": "(Balance, Balance, Balance)"
    }
  }
}
