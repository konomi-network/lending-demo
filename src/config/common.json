{
  "APP_NAME": "konomi-front-end-v2",
  "DEVELOPMENT_KEYRING": true,
  "RPC_REQUEST_URL": "https://dev.konomi.tech/rpc",
  "CUSTOM_TYPES": {
    "Address": "MultiAddress",
    "AssetId": "u64",
    "FeeRate": "u64",
    "LookupSource": "MultiAddress",
    "User": {
      "borrowLimit": "Balance",
      "supplyBalance": "Balance",
      "debtBalance": "Balance"
    },
    "Pool": {
      "enabled": "bool",
      "canBeCollateral": "bool",
      "asset": "AssetId",
      "supply": "Balance",
      "debt": "Balance",
      "safeFactor": "FixedU128",
      "closeFactor": "FixedU128",
      "discountFactor": "FixedU128",
      "totalSupplyIndex": "FixedU128",
      "totalDebtIndex": "FixedU128",
      "lastUpdated": "BlockNumber",
      "utilizationFactor": "FixedU128",
      "initialInterestRate": "FixedU128"
    },
    "UserSupply": {
      "amount": "Balance",
      "index": "FixedU128"
    },
    "UserDebt": {
      "amount": "Balance",
      "index": "FixedU128"
    }
  },
  "RPC": {
    "lending": {
      "supplyRate": {
        "description": "get asset supply rate",
        "isSubscription": "true",
        "jsonrpc": "lending_supplyRate",
        "method": "supplyRate",
        "params": [
          {
            "name": "id",
            "type": "AssetId"
          }
        ],
        "type": "FixedU128"
      },
      "debtRate": {
        "description": "get asset debt rate",
        "isSubscription": "true",
        "jsonrpc": "lending_debtRate",
        "method": "debtRate",
        "params": [
          {
            "name": "id",
            "type": "AssetId"
          }
        ],
        "section": "lending",
        "type": "FixedU128"
      },
      "getUserInfo": {
        "description": "get user info",
        "isSubscription": "true",
        "jsonrpc": "lending_getUserInfo",
        "method": "getUserInfo",
        "params": [
          {
            "name": "user",
            "type": "AccountId"
          }
        ],
        "section": "lending",
        "type": "(u64, u64, u64)"
      }
    }
  }
}
